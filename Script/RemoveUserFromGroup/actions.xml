<?xml version="1.0" encoding="utf-8"?>
<remv1:Remediation-Plugin xmlns:remv1="RemediationVersion1.xsd" Name="Remove User from Group">
  <remv1:Action Name="Remove User from Group" Command="powershell.exe">
    <remv1:ConstantParameter Name="Script" Switch="-file RemoveFromGroup.ps1" Order="1" />
    <remv1:StringParameter Name="Group" Switch="" Order="2" />
    <remv1:StringParameter Name="User" Switch="" Order="3" />
  </remv1:Action>
</remv1:Remediation-Plugin>